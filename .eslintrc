{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "rules": {
    "no-multiple-empty-lines": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "semi": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off"
  }
}
