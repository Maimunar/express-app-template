{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "extends": ["airbnb-base"],
  "plugins": ["jest"],
  "rules": {
    "no-multiple-empty-lines": "warn",
    "no-var": "error",
    "prefer-const": "error",
    "semi": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "import/prefer-default-export": "off",
    "import/no-unresolved": "off",
    "curly": "off",
    "nonblock-statement-body-position": "off"
  }
}
